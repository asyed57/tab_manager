{
    "name": "Tabs Manager",
    "description": "Tabs Manager",
    "version": "3.0",
    "permissions": [
        "tabs"
        , "<all_urls>"
        , "history"
        ,"webRequest"
        ,"bookmarks"
    ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Z",
                "mac": "MacCtrl+Z"
            }
        }
    },
    "browser_action": {
        "default_title": "Tabs Manager",
        "default_popup": "main.html",
        "default_icon": "icon.png"
    },
    "manifest_version": 2
}